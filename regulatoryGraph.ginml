      <annotation>
        <linklist>
          <link xlink:href="http://www.genenames.org/data/hgnc_data.php?hgnc_id=29259"/>
          <link xlink:href="http://www.genenames.org/data/hgnc_data.php?hgnc_id=16835"/>
          <link xlink:href="http://www.genenames.org/data/hgnc_data.php?hgnc_id=18133"/>
